<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - a</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
        integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous" />
    <link rel="stylesheet" href="/css/style.css" />
</head>

<body>
    <div class="container-md">
        <div class="row" id="topbar">
            <div class="col-12 d-flex flex-row justify-content-between">
                <div class="d-flex align-items-center">
                    <span>New York
                        London
                        Paris
                        Bogotá
                    </span>
                </div>
                <div>
                    <ul class="d-inline-flex h-100 align-items-center">
                        <li class="list-inline-item">
                            <a href="https://www.twitch.tv/rockstargames"><i class="fab fa-twitch"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="https://www.instagram.com/rockstargames"><i class="fab fa-instagram"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="https://twitter.com/rockstargames"><i class="fab fa-twitter"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="https://www.youtube.com/rockstargames"><i class="fab fa-youtube"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="https://www.facebook.com/rockstargames"><i class="fab fa-facebook"></i></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row" id="navbar">
            <nav class="navbar navbar-expand-lg bg-body-tertiary">
                <div class="container-fluid">
                    <a class="navbar-brand" href="https://www.rockstargames.com/">
                        <img src="/images/rs1.png" alt="Gta6" width="50" height="50" />
                    </a>
                    Rockstar Games
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" href="Home/About">About</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Home/Privacy">Link</a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-expanded="false">
                                    Games
                                </a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a class="dropdown-item" href="https://www.rockstargames.com/gta-v">grand theft
                                            auto V</a>
                                        <img src="/images/V.jpg" alt="" width="100" height="100" />
                                    </li>
                                    <li>
                                        <a class="dropdown-item" href="https://www.rockstargames.com/gta-online">grand
                                            theft auto ONLINE</a><img src="/images/GTAOnline.jpg" alt="" width="100"
                                            height="100" />
                                    </li>
                                    <li>
                                        <a class="dropdown-item" href="https://www.rockstargames.com/GTATrilogy">grand
                                            theft auto THE TRILOGY</a><img src="/images/GTATrilogy.jpg" alt=""
                                            width="100" height="100" />
                                    </li>
                                    <li>
                                        <a class="dropdown-item"
                                            href="https://www.rockstargames.com/reddeadredemption2">RED DEAD REDEMPTİON
                                            II</a><img src="/images/reddeadredemption2.jpg" alt="" width="100"
                                            height="100" />
                                    </li>
                                    <li>
                                        <a class="dropdown-item" href="https://www.rockstargames.com/reddeadonline">RED
                                            DEAD ONLINE</a>
                                    </li>
                                    <img src="/images/reddeadonline.jpg" alt="" width="100" height="100" />
                                </ul>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link disabled" aria-disabled="true">Disabled</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <a class="navbar-brand" href="https://pegi.info/">
                    <img src="/images/a380d3ce353544a356e8ce48f8586804.png" alt="Rockstar Games" width="50"
                        height="50" />
                </a>
            </nav>
        </div>
        @RenderBody()

        <div class="row" id="footer">
            <div class="col-2 col-lg-2 col-md-4 col-sm-4">
                <img src="/images/3.png" alt="" width="200px" height="200px" />
            </div>
            <div class="col-2 col-lg-2 col-md-4 col-sm-4">
                <img src="/images/VC.png" alt="" width="200px" height="200px" />
            </div>
            <div class="col-2 col-lg-2 col-md-4 col-sm-4">
                <img src="/images/SAN.jpg" alt="" width="200px" height="200px" />
            </div>
            <div class="col-2 col-lg-2 col-md-4 col-sm-4">
                <img src="/images/4.jpg" alt="" width="200px" height="200px" />
            </div>
            <div class="col-2 col-lg-2 col-md-4 col-sm-4">
                <img src="/images/lb.jpg" alt="" width="200px" height="200px" />
            </div>
            <div class="col-2 col-lg-2 col-md-4 col-sm-4">
                <img src="/images/V.jpg" alt="" width="200px" height="200px" />
            </div>
        </div>

    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
